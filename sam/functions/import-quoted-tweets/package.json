{
  "name": "@dog-eared/import-user-quoted-tweets",
  "version": "1.0.0",
  "main": "dist/bundle.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && parcel build src/index.ts --target main",
    "generate": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.15.4",
    "@graphql-codegen/introspection": "^1.15.4",
    "@graphql-codegen/typescript": "^1.15.4",
    "@graphql-codegen/typescript-document-nodes": "^1.16.3",
    "@graphql-codegen/typescript-operations": "^1.15.4",
    "@types/node-fetch": "^2.5.7",
    "parcel": "^2.0.0-beta.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6"
  },
  "browserslist": [
    "node 12"
  ],
  "targets": {
    "main": {
      "context": "node",
      "engines": {
        "node": "12.0"
      },
      "includeNodeModules": {
        "aws-sdk": false
      }
    }
  },
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "aws-sdk": "^2.711.0",
    "graphql": "^15.3.0",
    "node-fetch": "^2.6.0"
  }
}
